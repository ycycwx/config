{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@yotsubacy/config",
    "version": "2.2.0",
    "description": "config guide",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf eslint",
        "watch": "tsc -w",
        "lint": "eslint --max-warnings=0 .",
        "prebuild": "pnpm run clean",
        "prepublishOnly": "pnpm run build",
        "prepare": "husky && pnpm run build",
        "semantic-release": "semantic-release"
    },
    "files": [
        "eslint",
        "typescript.json"
    ],
    "exports": {
        "./eslint": "./eslint/index.js",
        "./package.json": "./package.json",
        "./typescript.json": "./typescript.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ycycwx/config.git"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ycycwx/config/issues"
    },
    "homepage": "https://github.com/ycycwx/config#readme",
    "dependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
        "@eslint/js": "^9.35.0",
        "@stylistic/eslint-plugin": "^5.3.1",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.4.0",
        "typescript-eslint": "^8.43.0"
    },
    "devDependencies": {
        "@commitlint/cli": "19.8.1",
        "@commitlint/config-conventional": "19.8.1",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "eslint": "9.35.0",
        "husky": "9.1.7",
        "lint-staged": "16.1.6",
        "semantic-release": "24.2.8",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "eslint": "^9.22.0"
    },
    "packageManager": "pnpm@10.17.1",
    "publishConfig": {
        "access": "public"
    }
}
